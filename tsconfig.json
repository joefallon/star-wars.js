{
  "compilerOptions": {
    "alwaysStrict": true,
    "downlevelIteration": true,
    "importHelpers": false,
    "inlineSourceMap": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "pretty": true,
    "skipLibCheck": true,
    "strictFunctionTypes": true,
    "target": "es5",
    "lib": [
      "es6",
      "dom",
      "scripthost"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}