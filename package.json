    "@types/react": "^16.4.12",
    "@types/react-loadable": "^5.4.1",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-table": "^6.7.12",
    "axios": "^0.18.0",
    "core-js": "^2.5.7",
    "lru-cache": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-table": "^6.8.6",
    "tslib": "^1.9.3"
  }
