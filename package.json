{
  "name": "@joefallon/star-wars.js",
  "version": "0.0.0",
  "description": "A Star Wars API explorer utilizing React, Webpack and friends.",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean && npm run webpack -- --config webpack.dev.js",
    "build:karma": "npm run clean && npm run webpack -- --config webpack.karma.js -w --watch-aggregate-timeout 250 --watch-poll --cache",
    "build:prod": "npm run clean && npm run webpack -- --config webpack.prod.js",
    "clean": "rm -rf public",
    "karma:run": "karma run",
    "mocha": "./node_modules/mocha/bin/mocha",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js --progress",
    "webpack": "./node_modules/webpack-cli/bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joefallon/star-wars.js.git"
  },
  "author": "Joe Fallon <joseph.t.fallon@gmail.com> (http://blog.joefallon.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joefallon/star-wars.js/issues"
  },
  "homepage": "https://github.com/joefallon/star-wars.js#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/karma": "^1.7.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.2",
    "@types/sinon": "^5.0.2",
    "@types/webpack": "^4.4.11",
    "@types/webpack-env": "^1.13.6",
    "assert": "^1.4.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "es6-promise": "^4.2.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mini-css-extract-plugin": "^0.4.2",
    "mocha": "^5.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "sinon": "^5.1.1",
    "source-map-loader": "^0.2.4",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.1.6",
    "webpack-merge": "^4.1.4",
    "worker-loader": "^2.0.0"
  },
